{
  "name": "xmtp",
  "version": "0.0.1",
  "private": true,
  "description": "Extensible ESMTP Server",
  "devDependencies": {
    "eslint": "^3.10.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "matcha": "^0.7.0",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "bench": "matcha --harmony ./packages/*/bench/*/*.js",
